<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MaiMaiApp.MainPage"
             NavigationPage.HasNavigationBar="False"
             NavigationPage.HasBackButton="False">

    <Grid Padding="40" RowSpacing="10" ColumnSpacing="10">

        <Grid.RowDefinitions>
            <RowDefinition Height="300"></RowDefinition>
        </Grid.RowDefinitions>

        <Button x:Name="LatestButton" Grid.Row="0" Grid.Column="0" FontSize="30" TextColor="White" BackgroundColor="SkyBlue" Text="최근 기록"></Button>
        <Button x:Name="SongDataButton" Grid.Row="0" Grid.Column="1" FontSize="30" TextColor="White" BackgroundColor="SkyBlue" Text="곡 정보"></Button>
    </Grid>

</ContentPage>
